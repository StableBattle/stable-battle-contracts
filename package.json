{
  "scripts": {
    "help": "npx hardhat help",
    "accounts": "npx hardhat accounts",
    "balances": "npx hardhat balances",
    "node:hh": "npx hardhat node",
    "clean": "npx hardhat clean",
    "test:hh": "npx hardhat test",
    "test:forge": "forge test -vvv --root . -c contracts --match-path contracts/test/* --out forge-artifacts",
    "coverage": "npx hardhat coverage",
    "compile": "npx hardhat compile",
    "build": "forge build --root . -c contracts --optimize --out forge-artifacts",
    "size": "npx hardhat size-contracts",
    "xdeploy": "npx hardhat xdeploy",
    "abi": "hardhat export-abi",
    "deploy:hh": "npx hardhat run --network hardhat scripts/deployStableBattle.ts",
    "deploy:localhost": "npx hardhat run --network localhost scripts/deployStableBattle.ts",
    "deploy:dashboard": "npx hardhat run --network truffle-dashboard scripts/deployStableBattle.ts",
    "deploy:tenderly": "npx hardhat run --network tenderly scripts/deployStableBattle.ts",
    "deploy:goerli": "npx hardhat run --network goerli scripts/deployStableBattle.ts",
    "deploy:ethmain": "npx hardhat run --network ethMain scripts/deployStableBattle.ts",
    "deploy:mumbai": "npx hardhat run --network mumbai scripts/deployStableBattle.ts",
    "deploy:polygon": "npx hardhat run --network polygon scripts/deployStableBattle.ts",
    "prettier:check": "prettier -c **/*.{js,ts,md,sol,json,yml}",
    "prettier:fix": "prettier -w **/*.{js,ts,md,sol,json,yml}",
    "solhint:check": "npx solhint 'contracts/**/*.sol'",
    "solhint:fix": "npx solhint 'contracts/**/*.sol' --fix",
    "lint:check": "yarn prettier:check && yarn solhint:check && npx eslint **/*.{js,ts}",
    "lint:fix": "yarn prettier:fix && yarn solhint:fix && npx eslint **/*.{js,ts} --fix"
  },
  "dependencies": {
    "@aave/core-v3": "^1.13.1",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.7.3",
    "@solidstate/contracts": "^0.0.41",
    "@tenderly/hardhat-tenderly": "^1.3.2",
    "dotenv": "^16.0.1",
    "hardhat": "^2.11.1",
    "hardhat-abi-exporter": "^2.10.1",
    "hardhat-contract-sizer": "^2.6.1"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.4.7",
    "@ethersproject/providers": "^5.4.7",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.3",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^1.0.1",
    "@nomiclabs/hardhat-ethers": "^2.1.1",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@solidstate/spec": "^0.0.41",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/chai": "^4.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^18.7.13",
    "chai": "^4.3.6",
    "ethers": "^5.7.1",
    "hardhat-gas-reporter": "^1.0.8",
    "prettier": "^2.7.1",
    "prettier-plugin-solidity": "^1.0.0-beta.24",
    "solidity-coverage": "^0.7.21",
    "ts-node": ">=8.0.0",
    "ts-transformer-keys": "^0.4.3",
    "typechain": "^8.1.0",
    "typescript": ">=4.5.0"
  }
}
